{
    "common": {
        "name":                 "fritzdect",
        "version":              "0.0.1",
        "title":                "fritzbox dect adapter",
        "desc":                 "ioBroker fritzdect Adapter",
        "platform":             "Javascript/Node.js",
        "mode":                 "daemon",
        "license": 	   	"MIT",
        "keywords":		["smart home", "DECT200", "fritzbox"],
        "type":			"hardware",
        "icon":                 "fritzdect_logo.png",
	"extIcon":          	"https://raw.githubusercontent.com/foxthefox/ioBroker.fritzdect/master/admin/fritzdect_logo.png",
	"readme":           	"https://github.com/foxthefox/ioBroker.fritzdect/master/README.md",
	"loglevel":		"debug"

    },
      "native": {
        "fritzbox-ip":  "192.x.x.x",
        "fritzbox-port": "9999"
    },
    "objects": [

    ],
    "instanceObjects": [
        {
            "_id": "power",
            "type": "state",
            "common": {
                "name":  "Lamp state",
                "type":  "boolean",
                "role":  "button",
                "read":  true,
                "write": true,
                "def":   null
            },
            "native": {
                "command": "lamp-state",
                "values": ["off","on","toggle","query"]
            }
        },
        {
            "_id": "connected",
            "type": "state",
            "common": {
                "name":  "Input selector",
                "type":  "string",
                "role":  "indicator",
                "read":  true,
                "write": false
            }
        }
    ]
}
